<!DOCTYPE html>
@using System.Web.Optimization
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Admin panel 2.0</title>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/jquery-ui.css" rel="stylesheet">
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <link href="~/Content/bootstrap-datetimepicker.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/plugins/bootstrap-combobox.css" rel="stylesheet">
    <link href="~/Content/plugins/Summernote/summernote.css" rel="stylesheet">
    <link href="~/Content/Site.css" rel="stylesheet">
    <link href="~/Content/plugins/BootSidePanel.css" rel="stylesheet">
    @Scripts.Render("~/bundles/modernizr")
               
    <script src="~/Scripts/jquery-2.1.4.js"> </script>
    <script src="~/Scripts/jquery-2.1.4.intellisense.js"> </script>
    <script src="~/Scripts/jquery.validate.js"> </script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.11.4.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/moment-with-locales.min.js"></script>
    <script src="~/Scripts/plugins/bootstrap-combobox.js"></script>
    <script src="~/Scripts/plugins/Summernote/summernote.min.js"></script>
    <script src="~/Scripts/bootstrap-datetimepicker.js"></script>
    <script src="~/Scripts/plugins/alerts/jquery.bsAlerts.js"></script>
    <script src="~/Scripts/BootSidePanel.js"></script>
    <script src="~/Scripts/Admin.js"></script>
    @Styles.Render("~/Content/themes/base/css")
</head>
<body>
    <div id="wrapper">
        <div class="">
            @Html.Action("NavbarTop", "Navbar")            
        </div>
        <div id="page-wrapper">
            <div id="sideNavbar" class="col-xs-2" style="height:100vh; background-color:#f8f8f8; border-color:#e7e7e7; ">
                @Html.Action("NavbarSide", "Navbar", new { controller = ViewContext.RouteData.Values["Controller"].ToString(), action = ViewContext.RouteData.Values["Action"].ToString() })
            </div>
            <div id="body" class="col-xs-10" >
               @RenderBody()
            </div>           
        </div>        

    </div>
    <!-- /#wrapper -->


 
    <script src="@Url.Content("~/Scripts/plugins/morris/raphael.min.js")"></script>
    <script src="@Url.Content("~/Scripts/plugins/morris/morris.min.js")"></script>
    <script src="@Url.Content("~/Scripts/plugins/morris/morris.js")"></script>
    <script src="@Url.Content("~/Scripts/plugins/morris/morris-data.js")"></script>
    
    <!-- Flot Charts JavaScript -->
    <!--[if lte IE 8]><script src="js/excanvas.min.js"></script><![endif]-->
    <script src="@Url.Content("~/Scripts/plugins/flot/jquery.flot.js")"></script>
    <script src="@Url.Content("~/Scripts/plugins/flot/jquery.flot.tooltip.min.js")"></script>
    <script src="@Url.Content("~/Scripts/plugins/flot/jquery.flot.resize.js")"></script>
    <script src="@Url.Content("~/Scripts/plugins/flot/jquery.flot.pie.js")"></script>
    <script src="@Url.Content("~/Scripts/plugins/flot/flot-data.js")"></script>
    <script type="text/javascript" src="~/Scripts/locales/bootstrap-datetimepicker.ru.js" charset="UTF-8"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
